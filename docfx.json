{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc",
      "templates/darkfx"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© 2019 Oscar Vasquez<br>Generated with <strong>DocFX</strong></span>",
      "_appTitle": "DocFX Material",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/ovasquez/docfx-material"
      },
      "_gitUrlPattern": "github"
    },
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}